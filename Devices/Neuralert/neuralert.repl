// SRAM window from mem.ld: 0x00080000 .. 0x000FFFFF
sram: Memory.MappedMemory @ sysbus 0x00080000
    size: 0x00080000

// CACHE execution window from mem.ld: 0x00100000 .. 0x002FFFFF
cache: Memory.MappedMemory @ sysbus 0x00100000
    size: 0x00200000

nvic: IRQControllers.NVIC @ sysbus 0xE000E000
    priorityMask: 0xF0

cpu: CPU.CortexM @ sysbus
    cpuType: "cortex-m4f"
    nvic: nvic
